		<point>org.openmrs.tracmodule.list</point>
		<class>@MODULE_PACKAGE@.extension.html.AdminList</class>
	</extension>
	 
	 <privilege>
      <name>View Data Quality</name>
       <description>Allows user to view data quality privileges</description>
    </privilege>


